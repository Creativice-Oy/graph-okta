{
  "name": "@jupiterone/graph-okta",
  "version": "1.6.8",
  "description": "A JupiterOne managed integration for https://www.okta.com",
  "main": "dist/index.js",
  "repository": "https://github.com/jupiterone/graph-okta",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config tools/execute.ts | bunyan",
    "start:containers": "docker-compose up -d",
    "stop:containers": "docker-compose down",
    "prebuild": "yarn test",
    "build": "rm -rf dist/* && yarn build:types && yarn build:src && yarn build:docs",
    "build:src": "babel src --extensions '.ts' --out-dir dist",
    "build:docs": "ts-node tools/docs.ts",
    "build:types": "tsc --project tsconfig.dist.json --declaration --emitDeclarationOnly",
    "lint": "yarn lint:ts && yarn lint:md",
    "lint:md": "remark .",
    "lint:ts": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest"
  },
  "dependencies": {
    "@jupiterone/jupiter-managed-integration-sdk": "^33.7.4",
    "@okta/okta-sdk-nodejs": "^2.0.0",
    "lodash.startcase": "^4.4.0",
    "promise-retry": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^7.0.0",
    "@types/gremlin": "^3.4.2",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "25",
    "@types/lodash.startcase": "^4.4.6",
    "@types/nock": "^10.0.3",
    "@types/node": "~8.10.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "bunyan": "^1.8.12",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^8.0.1",
    "husky": "^2.3.0",
    "jest": "25",
    "jest-haste-map": "25",
    "jest-resolve": "25",
    "lint-staged": "^8.1.3",
    "nock": "^10.0.6",
    "prettier": "2.0.5",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  }
}
